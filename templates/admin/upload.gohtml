{{ template "header" }}

<div class="portfolio">
    <div class="title port">
        <h1>Upload</h1>
    </div>
    <div class="addPic">
        <form action="" id="form" method="POST" enctype="multipart/form-data">
            <p style="margin-bottom: 5px;">Use .jpeg/.jpg files(for now)</p>
            <input type="file" name="file" id="file" class="inputfile" onchange="ch(event)"/>
            <label for="file">Choose a file</label>
        </form>
    </div>
    <div id="imgs">
    </div>
    <button type="submit" class="btn" onclick="upload(event)">Submit</button>
</div>

{{ template "footer" }}