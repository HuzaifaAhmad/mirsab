{{ template "header" }}

<div class="contact">
    <div class="title">
        <h1>Contacts Details</h1>
    </div>
    <div class="contactDetails">
        <div class="category">
            <h3>Name: </h3>
            <h3>Email: </h3>
            <h3>Subject: </h3>
            <h3>Sent At: </h3>
            <h3>Message: </h3>
        </div>
        <div class="vals">
            <p>{{ .Name }}</p>
            <p>{{ .Email }}</p>
            <p>{{ .Subject }}</p>
            <p>{{ .Time }}</p>
            <p>{{ .Msg }}</p>
        </div>
    </div>
</div>

{{ template "footer" }}